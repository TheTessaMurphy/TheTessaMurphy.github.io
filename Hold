const xmlhttp = new XMLHttpRequest();
xmlhttp.onload = function() {
  const myObj = JSON.parse(this.responseText);
  document.getElementById("demo").innerHTML = JSON.stringify(myObj.flash);
}
xmlhttp.open("GET", "Data/flashCards.json");
xmlhttp.send();

